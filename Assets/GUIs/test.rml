<rml>
	<head>
		<title>Rocket Test</title>
		<style>
			body{
				font-family: Delicious;
				font-weight: normal;
				font-style: normal;
				font-size: 16;
				color: white;
				background-color: #44444488;

				min-width: 120px;
				width: 500px;
				max-width: 750px;

				min-height: 100px;
				height: 120px;
				max-height: 250px;

				margin: 0px;
				padding: 6px;
			}

			button{
				background-color: #888888AA;
				color: white;
				font-weight: bold;
				padding: 2px 2px 2px 2px;
				margin: 0px 0px 0px 0px;
			}

			#title-bar{
				min-width: 100%;
				max-width: 100%;
				width: 100%;
				color: black;
				font-weight: bold;
				padding: 2px 2px 2px 2px;
				margin: 0px 0px 0px 0px;
				background-color: #FFFFFFAA;
			}

			input{
				border-color: #FFFFFFFF;
				border-width: 1px;
				padding: 2px 2px 2px 2px;
			}
		</style>
	</head>
	<body>
		<div id="title-bar">
			<handle move_target="#document">
				<div id="title">Rocket Panel</div>
			</handle>
		</div>
		<br/>

		Rocket Test<br/>
		<input type="text" id="model_path" />&nbsp;<button>Load Model</button>
	</body>
</rml>
